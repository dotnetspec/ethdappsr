 (cycleReplacer == null) cycleReplacer = function(key, value) {\n    if (stack[0] === value) return \"[Circular ~]\"\n    return \"[Circular ~.\" + keys.slice(0, stack.indexOf(value)).join(\".\") + \"]\"\n  }\n\n  return function(key, value) {\n    if (stack.length > 0) {\n      var thisPos = stack.indexOf(this)\n      ~thisPos ? stack.splice(thisPos + 1) : stack.push(this)\n      ~thisPos ? keys.splice(thisPos, Infinity, key) : keys.push(key)\n      if (~stack.indexOf(value)) value = cycleReplacer.call(this, key, value)\n    }\n    else stack.push(value)\n\n    return replacer == null ? value : replacer.call(this, key, value)\n  }\n}\n","name":"../../../.nvm/versions/node/v8.11.3/lib/node_modules/embark/node_modules/json-stringify-safe/stringify.js"}},"hash":"577c0ba7178cd30eee4c2af2e7b1a717","dependencyBlock":{"type":"DependenciesBlock","dependencies":[],"variables":[],"blocks":[]},"source":{"_cachedSource":{"source":{"type":"ReplaceSource","replacements":[]},"hash":"577c0ba7178cd30eee4c2af2e7b1a717"}}}{"type":"NormalModule","constructor":{"request":"../../../.nvm/versions/node/v8.11.3/lib/node_modules/embark/node_modules/ipfs-api/src/utils/stream-to-json-value.js","userRequest":"../../../.nvm/versions/node/v8.11.3/lib/node_modules/embark/node_modules/ipfs-api/src/utils/stream-to-json-value.js","rawRequest":"stream-to-json-value","loaders":[],"resource":"../../../.nvm/versions/node/v8.11.3/lib/node_modules/embark/node_modules/ipfs-api/src/utils/stream-to-json-value.js","parser":{"type":"Parser"}},"identifier":"../../../.nvm/versions/node/v8.11.3/lib/node_modules/embark/node_modules/ipfs-api/src/utils/stream-to-json-value.js","assigned":{"issuer":"../../../.nvm/versions/node/v8.11.3/lib/node_modules/embark/node_modules/ipfs-api/src/utils/send-request.js"},"build":{"built":false,"buildTimestamp":1534741029964,"cacheable":true,"meta":{},"assets":[],"fileDependencies":["../../../.nvm/versions/node/v8.11.3/lib/node_modules/embark/node_modules/ipfs-api/src/utils/stream-