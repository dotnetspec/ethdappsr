s[code]) {\n    return true\n  }\n\n  return false\n}\n\n/**\n * Check if the given buffer is a valid multihash. Throws an error if it is not valid.\n *\n * @param {Buffer} multihash\n * @returns {undefined}\n * @throws {Error}\n */\nfunction validate (multihash) {\n  exports.decode(multihash) // throws if bad.\n}\nexports.validate = validate\n\n/**\n * Returns a prefix from a valid multihash. Throws an error if it is not valid.\n *\n * @param {Buffer} multihash\n * @returns {undefined}\n * @throws {Error}\n */\nexports.prefix = function prefix (multihash) {\n  validate(multihash)\n\n  return multihash.slice(0, 2)\n}\n","name":"../../../.nvm/versions/node/v8.11.3/lib/node_modules/embark/node_modules/multihashes/src/index.js"}},"hash":"67ac75b0c87f4f55c1125444fb70eea7","dependencyBlock":{"type":"DependenciesBlock","dependencies":[{"type":"ConstDependency","expression":"","range":[75,87],"loc":{"start":{"line":6,"column":0},"end":{"line":6,"column":12}}},{"type":"CommonJsRequireDependency","request":"bs58","range":[110,116],"loc":{"start":{"line":8,"column":13},"end":{"line":8,"column":28}}},{"type":"RequireHeaderDependency","range":[102,109],"loc":{"start":{"line":8,"column":13},"end":{"line":8,"column":28}}},{"type":"CommonJsRequireDependency","request":"./constants","range":[138,151],"loc":{"start":{"line":10,"column":11},"end":{"line":10,"column":33}}},{"type":"RequireHeaderDependency","range":[130,137],"loc":{"start":{"line":10,"column":11},"end":{"line":10,"column":33}}},{"type":"CommonJsRequireDependency","request":"varint","range":[271,279],"loc":{"start":{"line":16,"column":15},"end":{"line":16,"column":32}}},{"type":"RequireHeaderDependency","range":[263,270],"loc":{"start":{"line":16,"column":15},"end":{"line":16,"column":32}}}],"variables":[{"type":"DependenciesBlockVariable","name":"Buffer","expression":"require(\"./../../node-libs-browser/node_modules/buffer/index.js\").Buffer","dependencies":[{"type":"CommonJsRequireDependency","request":"./../../node-l