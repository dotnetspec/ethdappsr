  var warned = false;\n  function deprecated() {\n    if (!warned) {\n      if (config('throwDeprecation')) {\n        throw new Error(msg);\n      } else if (config('traceDeprecation')) {\n        console.trace(msg);\n      } else {\n        console.warn(msg);\n      }\n      warned = true;\n    }\n    return fn.apply(this, arguments);\n  }\n\n  return deprecated;\n}\n\n/**\n * Checks `localStorage` for boolean values for the given `name`.\n *\n * @param {String} name\n * @returns {Boolean}\n * @api private\n */\n\nfunction config (name) {\n  // accessing global.localStorage can trigger a DOMException in sandboxed iframes\n  try {\n    if (!global.localStorage) return false;\n  } catch (_) {\n    return false;\n  }\n  var val = global.localStorage[name];\n  if (null == val) return false;\n  return String(val).toLowerCase() === 'true';\n}\n","name":"../../../.nvm/versions/node/v8.11.3/lib/node_modules/embark/node_modules/util-deprecate/browser.js"}},"hash":"55b755cce48523d0e3dd55c328d2fb59","dependencyBlock":{"type":"DependenciesBlock","dependencies":[],"variables":[{"type":"DependenciesBlockVariable","name":"global","expression":"require(\"./../webpack/buildin/global.js\")","dependencies":[{"type":"CommonJsRequireDependency","request":"./../webpack/buildin/global.js","range":[8,40],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":41}}},{"type":"RequireHeaderDependency","range":[0,7],"loc":{"start":{"line":1,"column":0},"end":{"line":1,"column":41}}}]}],"blocks":[]},"source":{"_cachedSource":{"source":{"type":"ReplaceSource","replacements":[[-9.5,-10.5,"/* WEBPACK VAR INJECTION */(function(global) {",0],[1615.5,1614.5,"\n/* WEBPACK VAR INJECTION */}.call(exports, __webpack_require__(7)))",1]]},"hash":"55b755cce48523d0e3dd55c328d2fb59"}}}{"type":"NormalModule","constructor":{"request":"../../../.nvm/versions/node/v8.11.3/lib/node_modules/embark/node_modules/readable-stream/lib/_stream_passthrough.js","userRequest":"../../../.nvm/versions/node/v8.11.3/lib